<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Usuario.
    -->
    <changeSet id="20181011212924-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="usuarios_id"
                                 baseTableName="usuario_likea"
                                 constraintName="fk_usuario_likea_usuarios_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>
