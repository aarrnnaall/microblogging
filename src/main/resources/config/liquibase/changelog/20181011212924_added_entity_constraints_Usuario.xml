                                 baseTableName="usuario_bloquea"
                                 constraintName="fk_usuario_bloquea_bloqueas_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>
        
    </changeSet>
</databaseChangeLog>
