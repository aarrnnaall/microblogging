                                 baseTableName="usuario_sigue"
                                 constraintName="fk_usuario_sigue_sigues_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>
        
        <addForeignKeyConstraint baseColumnNames="usuarios_id"
